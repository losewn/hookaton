{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package" : {
                "name" :    "dinbror/blazy",
                "version" : "v1.8.2",
                "type" :    "drupal-library",
                "source" : {
                    "url" : "https://github.com/dinbror/blazy",
                    "type" : "git",
                    "reference" : "master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "dinbror/blazy": "^1.8",
        "drupal/admin_toolbar": "^3.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/blazy": "^3.0",
        "drupal/block_class": "^2.0",
        "drupal/cleantalk": "^9.4",
        "drupal/core-composer-scaffold": "^9.5",
        "drupal/core-recommended": "^9.5",
        "drupal/ctools": "^4.1",
        "drupal/devel": "^5.1",
        "drupal/entity_reference_revisions": "^1.11",
        "drupal/exif_orientation": "^1.1",
        "drupal/externalauth": "^2.0",
        "drupal/facets": "^3.0@beta",
        "drupal/field_group": "^3.2",
        "drupal/fivestar": "^1.0@alpha",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_login": "^2.1",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/google_analytics": "^4.0",
        "drupal/google_api_client": "^4.3",
        "drupal/googlelogin-googlelogin": "^9.2",
        "drupal/layout_paragraphs": "^2.0",
        "drupal/like_dislike": "^2.1",
        "drupal/loggable": "^2.0",
        "drupal/metatag": "^2.0",
        "drupal/module_filter": "^4.1",
        "drupal/openid_connect": "^3.0@alpha",
        "drupal/paragraphs": "^1.17",
        "drupal/pathauto": "^1.9",
        "drupal/search_api": "^1.30",
        "drupal/sendinblue": "^2.0",
        "drupal/sitemap": "^2.0@beta",
        "drupal/smart_trim": "^2.1",
        "drupal/stage_file_proxy": "^1.2",
        "drupal/token": "^1.14",
        "drupal/twig_tweak": "^3.3",
        "drupal/twig_vardumper": "^3.1",
        "drupal/upgrade_status": "^3.14",
        "drupal/user_registrationpassword": "^2.0",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/vote_up_down": "^1.0@alpha",
        "drush/drush": "^10.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "cweagans/composer-patches": true,
            "drupal/core-project-message": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "nojs/ajax route parameter in use-ajax link breaks CSRF protection": "https://www.drupal.org/files/issues/drupal-exclude-csrf-paramaters-2670798-4.patch"
            },
            "sendinblue/api-v3-sdk": {
                "#3330960 - Deprecated function: Return type of SendinBlue\\Client\\Model\\SendSmtpEmailTo::offsetExists($offset)": "https://www.drupal.org/files/issues/2023-01-18/3330960-4-sendinblue_api_support_php81.patch"
            },
            "drupal/fivestar": {
                "User deprecated function: Theme functions are deprecated in drupal:8.0.0": "https://www.drupal.org/files/issues/2023-10-29/3256199_34.patch"
            }
        }
    },
    "require-dev": {
        "kint-php/kint": "^4.1"
    }
}
